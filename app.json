{
  "expo": {
    "name": "SMERF",
    "slug": "SMERF",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/avis-industries.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
